/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 400rpx;
}

.companyList{
  padding: 20rpx 20rpx 16rpx 20rpx;
  position: fixed;
  top:80rpx;
  width: 100%;
  background: white;
  border-top:1px solid #e8eaff;
  z-index: 9999;
}
.currentCompany{
  margin: 20rpx 0;
}
.companyList span{
  display: inline-block;
  width: 12rpx;
  height: 12rpx;
  border-radius: 6rpx;
  margin-right: 16rpx;
  background: #5552d9;
}
.tip{
  color:#3a51f9;
}
.item{
  margin-bottom: 20rpx;
  margin-left: 40rpx;
}
.selectedCompany{
  float: left;
  padding: 10rpx 20rpx;  
}
 .switch{
   float: left;  
}
 .exit{
  float: right;
}
.listIcon{
  width: 56rpx;
  height: 42rpx;
}
.nav{
  position: fixed;
  width: 100%;
  background: #3a51f9; 
  color: white;
  font-size: 30rpx;
  z-index: 999;
}

.section{
  text-align: center;
  padding:20rpx;
  padding-bottom:10rpx;
}
picker{
  display: inline-block;
}
.section span{
  font-size:32rpx;
  font-weight:600;
}
.section .picker text{
  color:#686868;
}
.onlineNum{
  height: 70rpx;
  font-size: 28rpx;
   background: #e8eaff; 
  color: #686d74;
  text-align: center;
  line-height: 70rpx;
}
.onlineNum text{
  color: #3a51f9;
}
.main{
  color: #2c3e50;
}
.interval {
    height: 30rpx;
    background: #f7f7f7;
}
.canvas{
  width: 100%;
  height: 400rpx;
}
.indic{
  padding: 20rpx 20rpx 10rpx 20rpx;
}
.indicIcon{
  width: 44rpx;
  height: 44rpx; 
}
.title{
  font-size:32rpx;
  font-weight:600;
  margin-left:10rpx;
  vertical-align:super;
}
.fortem{
  font-size: 32rpx;
  text-align:center;
  margin-bottom: 10rpx;
}
.doto{
  margin-bottom: 40rpx;
}